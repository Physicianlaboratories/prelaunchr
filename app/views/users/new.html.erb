<div class="index-wrap home-wrapper">
  <img class = "top-prod" src = "<%= asset_path('home/home-prod-1.png') %>" >
  <img class= "bottom-prod" src = "<%= asset_path('home/home-prod-2.png') %>" >
  <div class="container page-wrapper text-center">
    <div class="clearfix <%= "ended" if Rails.application.config.ended %>">
      <% if Rails.application.config.ended %>
        <%= render "users/campaign_ended" %>
      <% else %>
        <%= render "users/campaign_ongoing" %>
      <% end %>
    </div>
  </div>
</div>